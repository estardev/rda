<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="estar\rda\RdaBundle\Entity\Campo" table="campo">
    <indexes>
      <index name="fkCampoCategoria1Idx" columns="idCategoria"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nome" type="string" column="nome" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descrizione" type="string" column="descrizione" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipo" type="string" column="tipo" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="obbligatorioinserzione" type="boolean" column="obbligatorioInserzione" nullable="true"/>
    <field name="obbligatoriovalidazionetecnica" type="boolean" column="obbligatorioValidazioneTecnica" nullable="true"/>
    <field name="ordinamento" type="integer" column="ordinamento" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fieldset" type="text" column="fieldset" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="obbligatoriovalidazioneamministrativa" type="boolean" column="obbligatorioValidazioneAmministrativa" nullable="true"/>
    <field name="padre" type="string" column="padre" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dataattivazione" type="datetime" column="dataAttivazione" nullable="true"/>
    <field name="datadismissione" type="datetime" column="dataDismissione" nullable="true"/>
    <many-to-one field="idcategoria" target-entity="Categoria" fetch="LAZY">
      <join-columns>
        <join-column name="idCategoria" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
