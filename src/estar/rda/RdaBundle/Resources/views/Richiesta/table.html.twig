
        <thead>
            <tr>
                <th>Id Pratica</th>
                <th>Titolo</th>
                <th>Status</th>
                <th>Anno programmazione</th>
                <th>Numero protocollo</th>
                <th>Azioni</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}

            <tr data-state="richiesta">
               <td>{{ entity.id }}</td>
                <td>{{ entity.titolo }}</td>
                <td><button class="btn btn-primary" id="statoiter" data-id="{{ entity.id }}" ><span class="glyphicon glyphicon-list" aria-hidden="true"></span> {{ entity.status }} {% if(entity.statusgestav!="") %}  -  {{ entity.statusgestav }} {% endif %} </button></td>                <td>{{ entity.numeroprotocollo }}</td>
                <td>{{ entity.annoprogrammazione }}</td>
                <td>{{ entity.numeroprotocollo }}</td>
                <td>

                    <div class="btn-group" role="group">
                        <button type="button" class="btn btn-default"><span class="glyphicon glyphicon-file" aria-hidden="true"></span><a target="_blank" href="{{ path('formtemplate_print', { 'idRichiesta': entity.id, 'idCategoria': entity.idcategoria }) }}"> Genera PDF</a></button>
                        <button type="button" class="btn btn-default"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span><a href="{{ path('formtemplate_edit', { 'idRichiesta': entity.id ,'idCategoria': entity.idcategoria}) }}"> Modifica</a></button>
                        <button type="button" class="btn btn-default"><span class="glyphicon glyphicon-book" aria-hidden="true"></span><a href="{{ path('documento_byCategoria', { 'idRichiesta': entity.id ,'idCategoria': entity.idcategoria}) }}"> Documenti</a></button>
                    </div>

                </td>
            </tr>
        {% endfor %}
        </tbody>